{
    "manifest_version": 2,
    "name": "Sensitive Images Sniffer",
    "description": "贴吧 imis 后台敏感图片地址归档",
    "version": "1.0.0",
    "permissions": [
        "http://*.baidu.com/",
        "unlimitedStorage",
        "tabs",
        "background"
    ],
    "icons": {
        "256": "img/icons/icon_32.png",
        "128": "img/icons/icon_32.png",
        "64": "img/icons/icon_32.png",
        "32": "img/icons/icon_32.png",
        "16": "img/icons/icon_16.png"
    },
    "browser_action": {
        "default_icon": "favicon.png",
        "name": "Sensitive Images Sniffer"
    },
    "web_accessible_resources": [
        "favicon.png"
    ],
    "background": {
        "page": "back.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}